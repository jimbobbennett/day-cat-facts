{
  "sdkName": "CatFacts",
  "specFilePath": "https://catfact.ninja/docs/api-docs.json",
  "languages": [
    "csharp"
  ],
  // The cat facts API uses an API key for authentication, but seems to work without a key set
  "auth": [
    "apikey"
  ],
  "customizations": {
    "authentication": {
      "apiKey": {
        "header": "X-CSRF-TOKEN"
      }
    }
  },
  // The API spec doesn't define the servers, so we need to provide the base URL
  "baseUrl": "https://catfact.ninja",
  "languageOptions": {
    "csharp": {
      "packageId": "liblab.Examples.CatFacts",
      "liblabVersion": "2",
      "sdkVersion": "0.0.1"
    }
  },
  // The API spec has some warnings that we can ignore
  "validationsToIgnore": [
    {
      "code": "oas3-api-servers"
    },
    {
      "code": "info-contact"
    },
    {
      "code": "info-description"
    },
    {
      "code": "operation-tag-defined"
    }
  ]
}